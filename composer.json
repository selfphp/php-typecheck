{
  "name": "selfphp/php-typecheck",
  "description": "Runtime type validation for arrays and structured data. in PHP",
  "keywords": ["typecheck", "type checking", "validation", "primitive", "assert", "utility", "selfphp"],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Selfphp\\PhpTypeCheck\\": "src/PhpTypeCheck/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Selfphp\\TypeCheck\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.1"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
