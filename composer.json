{
  "name": "selfphp/php-typecheck",
  "description": "Runtime type checking for arrays and nested structures in PHP",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Selfphp\\PhpTypeCheck\\": "src/PhpTypeCheck/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Selfphp\\TypeCheck\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.1"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
